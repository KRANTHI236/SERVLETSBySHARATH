<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>SignupPage</servlet-name>
		<servlet-class> Controller.Servlet_Class_For_Signup</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>SignupPage</servlet-name>
		<url-pattern>/Form1</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>LoginPage</servlet-name>
		<servlet-class>Controller.Servlet_Class_For_Login</servlet-class>

	</servlet>

	<servlet-mapping>
		<servlet-name>LoginPage</servlet-name>
		<url-pattern>/Form2</url-pattern>

	</servlet-mapping>

</web-app>
